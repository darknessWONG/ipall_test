<h1>All employees</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <% @employee.each do |employee| %>
    <tr>
      <td><%= employee.name %></td>
      <td><%= link_to 'show', employee %></td>
      <td><%= link_to 'edit', edit_employee_path(employee) %></td>
      <td><%= link_to 'delete', employee_path(employee), method: :delete, data: {confirm: 'Are your sure?'} %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New employee', new_employee_path %>
<br />
<%= link_to 'upload', upload_path %>
<br />
<%= link_to 'Download List', downloadlist_path %>

