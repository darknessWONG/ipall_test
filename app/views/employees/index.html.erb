<%= javascript_include_tag "jquery.min" %>
<%= javascript_include_tag "jquery_ujs" %>
<h1>All employees</h1>
  <div id="user_nav">
    <% if user_signed_in? %>Signed in as <%= current_user.email %>. Not you?
    <%= link_to "Sign out", destroy_user_session_path, method: :delete, data: {confirm: 'Are your sure?'} %>
    <% else %>
    <%= link_to "Sign up", new_user_registration_path %> or<%= link_to "Sign in", new_user_session_path %>
    <% end %>
  </div> 
<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  <% @employee.each do |employee| %>
    <tr>
      <td><%= employee.name %></td>
      <td><%= link_to 'show', employee %></td>
      <td><%= link_to 'edit', edit_employee_path(employee) %></td>
      <td><%= link_to 'delete', employee_path(employee), method: :delete, data: {confirm: 'Are your sure?'} %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New employee', new_employee_path %>
<br />
<%= link_to 'upload', upload_path %>
<br />
<%= link_to 'Download List', downloadlist_path %>
<br />
<%= link_to 'Sign out', destroy_user_session_path %>
<br />
<%= link_to 'out', destroy_user_session_path, method: :delete, data: {confirm: 'Are your sure?'} %>
