<h1> download list </h1>

<table>
	<tr>
		<th>name</th>
		<th></th>
	</tr>
	<% @list.each do |file| %>
		<% if file != '.' && file !='..' %>
			<tr>
				<td><%= file %></td>
				<td><%= link_to 'download', "/download/#{file}"%></td>
			</tr>
		<% end %>
	<% end %>
</table>

<%= link_to 'home', root_path %>
